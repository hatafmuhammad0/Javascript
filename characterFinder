//Write a function that takes an array of strings and returns a new array with only the strings that contain the letter 'a'.

let sen = ["apple", "link", "grapes", "jokese"]

function findCharc(arr,character) {

    let newArray = []

    arr.forEach((element) => {
        if (element.includes(character)) {
            newArray.push(element)
        }
    })

    return newArray

}

console.log(findCharc(sen, "i"))
